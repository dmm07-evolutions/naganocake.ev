<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h2>登録情報編集</h2>
    </div>
  </div>
  <div class="row">
    <div class="col-md-offset-2 col-md-10">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-group row">
          <%= f.label :email, "メールアドレス", class:"col-md-2 control-label" %>
          <div class="col-md-6">
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
          </div>
      </div>

      <div class="form-group row">
          <%= f.label :password, class:"col-md-2 control-label" %>
          <div class="col-md-6">
          <%= f.password_field :password, autocomplete: "current-password", class:"form-control" %>
          </div>
      </div>

      <div class="actions">
        <%= f.submit "ログイン", class:"btn btn-primary col-md-offset-3" %>
      </div>
    <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      <%= link_to "編集内容を保存する", "", class:"btn btn-primary" %>
      <%= link_to "退会する", "", class:"btn btn-danger" %>
    </div>
  </div>
</div>
